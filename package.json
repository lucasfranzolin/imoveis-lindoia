{
    "name": "imoveis-lindoia",
    "version": "1.0.0",
    "description": "",
    "type": "commonjs",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.+(json|ts)": [
            "node_modules/.bin/eslint --fix",
            "node_modules/.bin/prettier --write .",
            "node_modules/.bin/jest"
        ]
    },
    "scripts": {
        "lint-staged": "lint-staged",
        "compile": "node_modules/.bin/swc src -d dist",
        "clean": "rm -rf dist",
        "build": "npm run clean && npm run compile",
        "lint": "eslint --ext .ts .",
        "test": "jest",
        "dev": "NODE_ENV=development node_modules/.bin/ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
        "start": "npm run build && node dist/index.js",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.208",
        "@swc/helpers": "^0.4.3",
        "@swc/jest": "^0.2.21",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/formidable": "^2.0.5",
        "@types/jest": "^28.1.4",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.0.0",
        "@types/nodemailer": "^6.4.4",
        "@typescript-eslint/eslint-plugin": "^5.30.3",
        "@typescript-eslint/parser": "^5.30.3",
        "eslint": "^8.19.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^8.0.0",
        "jest": "^28.1.2",
        "lint-staged": "^13.0.3",
        "node-mocks-http": "^1.11.0",
        "prettier": "2.7.1",
        "regenerator-runtime": "^0.13.9",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "aws-sdk": "^2.1181.0",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.5",
        "deep-copy": "^1.4.2",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "formidable": "^2.0.1",
        "helmet": "^5.1.0",
        "http-status": "^1.5.2",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.7.0",
        "morgan": "^1.10.0",
        "nodemailer": "^6.7.7",
        "rxjs": "^7.5.6",
        "winston": "^3.8.1"
    }
}
